#!/bin/bash
. bin/db.inc
TMPFILE=/tmp/hard_cache.$$

for seance_id in $(echo "select DISTINCT id from seance where type = 'hemicycle'" | mysql $MYSQLID $DBNAME | grep -v id )
do
	echo "http://www.nosdeputes.fr/seance/$seance_id"
	rm web/seance/$seance_id.html 2> /dev/null
	curl -q http://www.nosdeputes.fr/seance/$seance_id > TMPFILE
	mv TMPFILE web/seance/$seance_id.html
done
rm cache/frontend/prod/template/www_nosdeputes_fr/all/intervention/seance/seance/*
rm cache/frontend/prod/template/www_nosdeputes_fr/all/sf_cache_partial/plot/_groupes/sf_cache_key/*

